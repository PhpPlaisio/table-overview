{
  "name": "plaisio/table-overview",
  "description": "PhpPlaisio: Overview Table",
  "keywords": [
    "PhpPlaisio",
    "Overview Table"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/babel": "^2.0",
    "plaisio/helper-css": "^1.1",
    "plaisio/helper-html": "^1.1",
    "plaisio/kernel": "^3.0",
    "plaisio/web-assets": "^2.0",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Table\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
