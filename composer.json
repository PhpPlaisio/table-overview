{
  "name": "plaisio/table-overview",
  "description": "PhpPlaisio: Overview Table",
  "keywords": [
    "PhpPlaisio",
    "Overview Table"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio-ts/helper-cast": "^2.0.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/babel": "^2.2.0",
    "plaisio/helper-css": "^1.3.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/kernel": "^3.3.0",
    "plaisio/web-assets": "^3.2.0",
    "setbased/exception": "^2.6.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-assets": "^1.3.0",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/console-type-script": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Table\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
